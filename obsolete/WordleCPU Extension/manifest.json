{
    "name": "Wordle CPU",
    "description": "Wordle CPU",
    "version": "1.0",
    "author": "Duncan Lynn and Derek Zhang",
    "manifest_version": 3,
    "permissions": ["scripting", "tabs"],
    "content_scripts": [
        {
            "matches": ["https://www.nytimes.com/games/wordle/*", "https://wordle5.com/"],
            "js": ["jquery.min.js","wordle.js"]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    }
}